        "code" : "dfgEntry {
  result.resize(0);
  Scalar size = 5.0;
  for(UInt32 k=0; k<numXfos; k++) {
      Scalar t =  Scalar(k)/ Scalar(Math_max(1,(numXfos-1)));
      Xfo outXfo = Xfo(
              this.evalPosition(t,keepArcLen,keepCurveLen),
              this.evalOri(t,keepArcLen,keepCurveLen),
