
struct KrakenSolverArg {
    String name;
    String connectionType;
    String dataType;
    String defaultValue;
};

// Default Constructor
inline KrakenSolverArg()
{

}

inline KrakenSolverArg(
  String name,
  String connectionType,
  String dataType)
{
  this.name = name;
  this.connectionType = connectionType;
  this.dataType = dataType;
  this.defaultValue = "";
}

inline KrakenSolverArg(
  String name,
  String connectionType,
  String dataType,
  String defaultValue)
{
  this.name = name;
  this.connectionType = connectionType;
  this.dataType = dataType;
  this.defaultValue = defaultValue;
}