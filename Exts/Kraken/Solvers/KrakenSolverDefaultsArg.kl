
struct KrakenSolverDefaultsArg {
    String name;
    String connectionType;
    String dataType;
    String defaultValue;
};


// Default Constructor
inline KrakenSolverDefaultsArg()
{

}

inline KrakenSolverDefaultsArg(
  String name,
  String connectionType,
  String dataType,
  String defaultValue)
{
  this.name = name;
  this.connectionType = connectionType;
  this.dataType = dataType;
  this.defaultValue = defaultValue;
}